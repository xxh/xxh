#!/usr/bin/env python3

import os, sys
from shutil import which
from xxh_xxh.xxh import xxh, eeprint

if __name__ == '__main__':
    if not which('ssh'):
        eeprint('Install OpenSSH client before using xxh: https://duckduckgo.com/?q=how+to+install+openssh+client+in+linux')

    xxh = xxh()
    xxh.main()
